{
  "expo": {
    "name": "react-native-element tooltip",
    "slug": "rneTooltip",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Custom component based on Tooltip from react-native-elements to vary height based on tooltip's text length.",
    "githubUrl": "https://github.com/tingzhouu/rn-rne-tooltip"
  }
}
